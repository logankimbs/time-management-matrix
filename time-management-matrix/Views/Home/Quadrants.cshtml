@{
    ViewData["Title"] = "Quadrants";
}

<table class="table table-bordered table-striped">
    <thead>
        <div class="row">

            <div class="col">
                <h1>Stephen Covey's Time Management Matrix</h1>
            </div>

        </div>
    </thead>
    <tbody>
        <div class="row">
            <div class="col-2">
            </div>
            <div class="col-5">
                <h3>Urgent</h3>
            </div>
            <div class="col-5">
                <h3>Not Urgent</h3>
            </div>
        </div>

        <div class="row">
            <div class="col-2">
                <h3>Important</h3>
            </div>
            <div class="col-5">
                <h5>Quadrant 1</h5>
            </div>
            <div class="col-5">
                <h5>Quadrant 2</h5>
            </div>
        </div>

        <div class="row">
            <div class="col-2">
                <h3>Not Important</h3>
            </div>
            <div class="col-5">
                <h5>Quadrant 3</h5>
            </div>
            <div class="col-5">
                <h5>Quadrant 4</h5>
            </div>
        </div>
    </tbody>
